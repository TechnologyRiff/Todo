<div class="container">
  <div class="row">
    <div class="col-md-8">
      <% if current_user.list.present? %>
            <%= render partial: 'lists/list' %>
        <% elsif current_user.present? && @list.nil? %>
          <%= link_to "New To-do List", new_list_path, class: 'btn btn-success' %>
        <% else %>
          <%= render 'signup' %>
      <% end %>
    </div>
  </div>
</div>

    
