<div class="row">
  <div class="col-md-8">
    <% if @list.present? %>
      <h1><%= @list.title %></h1>
    <% else %>
        <div class="col-md-8">
          <% if policy(List.new).create? %>
            <%= link_to "Create List", new_list_path(@list), class: 'btn btn-success' %>
          <% end %>
        </div>
    <% end %>
  </div>
</div>
<div class="col-md-4">
</div>


